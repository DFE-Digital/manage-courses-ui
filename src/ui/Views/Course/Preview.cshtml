@model CourseReferenceViewModel
 @{
  Layout = "_Layout";
}

<!-- CourseDetails is a dependency from SearchAndCompareUi.Shared -->
@await Component.InvokeAsync("CourseDetails", new { 
  providerCode = Model.InstCode,
  courseCode = Model.CourseCode,
  preview = true,
  aboutYourOrgLink = Url.Action("About", "Organisation", new { ucasCode = Model.InstCode }) })