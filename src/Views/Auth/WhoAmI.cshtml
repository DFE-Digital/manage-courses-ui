@model ManageCoursesUi.Controllers.WhoAmIViewModel

<h2>Who Am I?</h2>

<h3>Raw Claims</h3>
<table>
    <thead>
        <tr>
            <th>Type</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model.RawClaims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>
<h3>Organisation</h3>
<dl>
    <dt>id</dt>
    <dd>@Model.Org.id</dd>
    <dt>name</dt>
    <dd>@Model.Org.name</dd>
    <dt>category id</dt>
    <dd>@Model.Org.category.id</dd>
    <dt>category name</dt>
    <dd>@Model.Org.category.name</dd>
</dl>
